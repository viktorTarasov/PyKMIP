            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>

	<wsdl:operation name="version">
            <wsdl:documentation> Returns version information about the Rights connnector. </wsdl:documentation>
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/version"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>

    </wsdl:binding>
    <wsdl:service name="OTPKIRightsConnectorSOAPService">
        <wsdl:documentation> Connector To access the OpenTrust PKI Rights
Management System via SOAP </wsdl:documentation>
        <wsdl:port name="OTPKIRightsConnectorSOAPPort" binding="pkirightsconn:OTPKIRightsConnectorSOAPBinding">
            <soap:address location="https://servernotconfigured:8080/rights/connector.cgi"/>
        </wsdl:port>
    </wsdl:service>
