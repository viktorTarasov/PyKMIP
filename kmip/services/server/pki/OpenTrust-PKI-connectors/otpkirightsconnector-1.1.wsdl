    <wsdl:types>
        <wsdl:documentation>Types used by the OpenTrust PKI SOAP Connector</wsdl:documentation>
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
            targetNamespace="http://www.opentrust.com/OpenTrust/XML/Message/1.2">
            <xs:annotation>
                <xs:documentation> External definition of the
        OpenTrust Message schema </xs:documentation>
            </xs:annotation>
            <xs:include schemaLocation="otmessage-1.2.xsd"/>
        </xs:schema>

    </wsdl:types>
    <wsdl:message name="OTMessageIn">
        <wsdl:documentation>Input message used for every OpenTrust PKI Rights Connector operation : contains one
            argument : an OTMessage </wsdl:documentation>
        <wsdl:part name="InputParameter" type="otmsg:OTMessageType"/>
    </wsdl:message>
    <wsdl:message name="OTMessageOut">
        <wsdl:documentation>Output message used for every OpenTrust PKI Rights Connector operation : contains one
            argument : an OTMessage</wsdl:documentation>
        <wsdl:part name="OutputParameter" type="otmsg:OTMessageType"/>
    </wsdl:message>
    <wsdl:portType name="OTPKIRightsConnectorSOAPPortType">
        <wsdl:operation name="listGroups">
            <wsdl:documentation>List existing groups </wsdl:documentation>
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
        <wsdl:operation name="getGroups">
            <wsdl:documentation>Get the list of groups a given certificate belongs to.</wsdl:documentation>
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
        <wsdl:operation name="getGroupMembers">
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
        <wsdl:operation name="addDNToGroup">
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
        <wsdl:operation name="removeDNFromGroup">
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
        <wsdl:operation name="addGroupToGroup">
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
        <wsdl:operation name="removeGroupFromGroup">
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
        <wsdl:operation name="version">
            <wsdl:input message="pkirightsconn:OTMessageIn"/>
            <wsdl:output message="pkirightsconn:OTMessageOut"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="OTPKIRightsConnectorSOAPBinding" type="pkirightsconn:OTPKIRightsConnectorSOAPPortType">
        <wsdl:documentation> SOAP Binding for the OpenTrust PKI Rights Connector </wsdl:documentation>
        <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="listGroups">
            <wsdl:documentation>List existing groups. </wsdl:documentation>
            <soap:operation
                soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/listGroups"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>
        <wsdl:operation name="getGroups">
            <wsdl:documentation>Get the list of groups a given certificate belongs to.</wsdl:documentation>
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/getGroups"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>
        <wsdl:operation name="getGroupMembers">
            <wsdl:documentation> Return the list of members for a given group. </wsdl:documentation>
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/getGroupMembers"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>
	    
	<wsdl:operation name="addDNToGroup">
            <wsdl:documentation> Add a certificate to a group. </wsdl:documentation>
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/addDNToGroup"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>

	<wsdl:operation name="removeDNFromGroup">
            <wsdl:documentation> Remove a certificate from a group. </wsdl:documentation>
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/removeDNFromGroup"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>

	<wsdl:operation name="addGroupToGroup">
            <wsdl:documentation> Add a group to another group. </wsdl:documentation>
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/addGroupToGroup"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1"/>
            </wsdl:output>
        </wsdl:operation>

	<wsdl:operation name="removeGroupFromGroup">
            <wsdl:documentation> Remove a group from another group. </wsdl:documentation>
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/Rights/SOAP/1.1/removeGroupFromGroup"
