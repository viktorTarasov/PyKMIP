                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/SOAP/2.2"/>
            </wsdl:output>
        </wsdl:operation>
	<wsdl:operation name="search_for_certificates">
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/SOAP/2.2/search_for_certificates"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/SOAP/2.2"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/SOAP/2.2"/>
            </wsdl:output>
        </wsdl:operation>
	<wsdl:operation name="retrieve_cert_authorities">
            <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/SOAP/2.2/retrieve_cert_authorities"
                style="rpc"/>
            <wsdl:input>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/SOAP/2.2"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
                    namespace="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/SOAP/2.2"/>
            </wsdl:output>
        </wsdl:operation>
	<wsdl:operation name="retrieve_pkcs12">
	  <soap:operation soapAction="http://www.opentrust.com/OpenTrust/PKI/Connector/EE/2.2/retrieve_pkcs12" style="rpc"/>
	  <wsdl:input>
		    <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" use="literal"
