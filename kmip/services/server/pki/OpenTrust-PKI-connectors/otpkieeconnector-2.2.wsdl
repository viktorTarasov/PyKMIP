    </wsdl:binding>
    <wsdl:service name="OTPKIEEConnectorSOAPService">
        <wsdl:documentation> SOAP service To access the OpenTrust PKI EE Connector via SOAP </wsdl:documentation>
        <wsdl:port name="OTPKIEEConnectorSOAPPort" binding="pkieeconn:OTPKIEEConnectorSOAPBinding">
            <soap:address location="https://kmip-pki.int.opentrust.com/EE/connector.cgi"/>
